{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "object-curly-spacing": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "operator-linebreak": "off",
        "no-debugger": "off",
        "indent": "off"
    }
}